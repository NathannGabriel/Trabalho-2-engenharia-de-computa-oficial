c1 = c2 = c3 = 0
total = int(input("Total de eleitores: "))

for i in range(1, total + 1):
    voto = int(input(f"Eleitor {i}, vote (1, 2 ou 3): "))
    if voto == 1:
        c1 += 1
    elif voto == 2:
        c2 += 1
    elif voto == 3:
        c3 += 1
    else:
        print("Voto inv√°lido.")

print("Resultado final:")
print("Candidato 1:", c1, "votos")
print("Candidato 2:", c2, "votos")
print("Candidato 3:", c3, "votos")
