matriz = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
]

soma_principal = 0
soma_secundaria = 0
n = len(matriz)

for i in range(n):
    soma_principal += matriz[i][i]
    soma_secundaria += matriz[i][n - 1 - i]

print("Soma diagonal principal:", soma_principal)
print("Soma diagonal secund√°ria:", soma_secundaria)