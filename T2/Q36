num = int(input("Montar a tabuada de: "))
inicio = int(input("ComeÃ§ar por: "))
fim = int(input("Terminar em: "))

if fim < inicio:
    print("Erro: o valor final deve ser maior ou igual ao inicial.")
else:
    print(f"Tabuada de {num} de {inicio} a {fim}:")
    for i in range(inicio, fim + 1):
        print(f"{num} x {i} = {num * i}")